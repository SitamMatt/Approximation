<UserControl x:Class="Aproksymacja.ApproximationPlot"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Aproksymacja"
             xmlns:lvp="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <lvp:CartesianChart>
            <lvp:CartesianChart.Series>
                <lvp:LineSeries x:Name="Function"></lvp:LineSeries>
                <lvp:LineSeries x:Name="ApproximatedFunction"></lvp:LineSeries>
            </lvp:CartesianChart.Series>
        </lvp:CartesianChart>
    </Grid>
</UserControl>
